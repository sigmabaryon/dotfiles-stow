abbr --add dotdot --regex '^\.\.+$' --function multicd
abbr --add ls 'eza -al --color=always --group-directories-first --icons'
abbr --add la 'eza -a --color=always --group-directories-first --icons'
abbr --add ll 'eza -l --color=always --group-directories-first --icons'
abbr --add lt 'eza -aT --color=always --group-directories-first --icons'
abbr --add l. "eza -a | grep -e '^\.'"
